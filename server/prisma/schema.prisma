// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prima-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
//migration ferramenta para manipular banco de dados, arquivos que pode alterar o banco
model Pool {
  id String @id @default(cuid())
  title String
  code String @unique
  createdAt DateTime @default(now())
}
